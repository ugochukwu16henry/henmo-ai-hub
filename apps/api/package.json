{
  "name": "henmo-ai-api",
  "version": "1.0.0",
  "description": "Henmo AI API Server",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/",
    "migrate": "node scripts/migrate.js",
    "migrate:rollback": "node scripts/rollback.js",
    "seed": "node scripts/seed.js",
    "docker:build": "docker build -t henmo-ai-api .",
    "docker:run": "docker run -p 3001:3001 henmo-ai-api"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "@anthropic-ai/sdk": "^0.9.1",
    "openai": "^4.20.1",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "nodemailer": "^6.9.7",
    "stripe": "^14.7.0",
    "ws": "^8.14.2",
    "@sentry/node": "^7.81.1",
    "winston": "^3.11.0",
    "multer-s3": "^3.0.1",
    "aws-sdk": "^2.1498.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "@types/jest": "^29.5.8"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}